NAME "Very large Czech corpus"
PATH /opt/lindat/kontext-data/corpora/data/monolingual/czes/czes_cs-w
VERTICAL "/opt/lindat/kontext-data/corpora/vert/monolingual/czes/czes_cs-w"
ENCODING utf-8
INFO "First version of the very large Czech corpus Czes created with a new set of tools in Masaryk university. It comprises 465,102,710 tokens."
LANGUAGE "Czech"

SHORTREF ""
DOCSTRUCTURE doc

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "cs_CS.UTF-8"
}
ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "cs_CS.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}

STRUCTURE doc {
    ATTRIBUTE xdedupl_id
    ATTRIBUTE lang
    ATTRIBUTE t_main
    ATTRIBUTE medium
    ATTRIBUTE title
    ATTRIBUTE auth_n
    ATTRIBUTE d_dupl
    ATTRIBUTE source
    ATTRIBUTE m_date
    ATTRIBUTE m_auth
    ATTRIBUTE m_desc
    ATTRIBUTE t_sub
    ATTRIBUTE t_orig
    ATTRIBUTE wordcount
}

STRUCTURE p {
    DISPLAYTAG 0
}

STRUCTURE s {
    DISPLAYTAG 0
}

STRUCTURE caption {
    DISPLAYTAG 0
    DISPLAYBEGIN "_EMPTY_"
}

STRUCTURE head {
    DISPLAYTAG 0
    DISPLAYBEGIN "_EMPTY_"

}

STRUCTURE g {
    DISPLAYTAG 0
    DISPLAYBEGIN "_EMPTY_"
}

STRUCTURE l {
    DISPLAYTAG 0
    DISPLAYBEGIN "_EMPTY_"
}

STRUCTURE gap {
    ATTRIBUTE tokens
    DISPLAYTAG 0
    DISPLAYBEGIN "_EMPTY_"
}


MAXCONTEXT 50
MAXDETAIL 50

