        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "en_AG.utf8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
        LOCALE "en_AG.utf8"
}


