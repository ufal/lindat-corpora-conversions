    DYNAMIC utf8lowercase
    DYNLIB internal
    FUNTYPE s
    ARG1 "it_CH.utf8"
    FROMATTR word
    TYPE index
    TRANSQUERY yes
    LOCALE "it_CH.utf8"
}


