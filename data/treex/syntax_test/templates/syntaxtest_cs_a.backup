NAME "Testing syntax annotation"
PATH "/opt/projects/lindat-services-kontext/devel/data/corpora/data/monolingual/test_syntax"
VERTICAL "/opt/projects/lindat-services-kontext/devel/data/corpora/vert/monolingual/test_syntax/testsyntax_cs-a"
ENCODING utf-8
INFO "5792 sentences from cwc parsed up to analytical layer. The attributes are: node: form, lemma, tag, afun, parent: p_form, p_lemma, p_tag, p_afun"
LANGUAGE "Czech"

SHORTREF ""
TAGSETDOC "http://ucnk.ff.cuni.cz/bonito/znacky.php"
WPOSLIST ",podstatné jméno,N.*,přídavné jméno,A.*,zájmeno,P.*,číslovka,C.*,sloveso,V.*,příslovce,D.*,předložka,R.*,spojka,J.*,částice,T.*,citoslovce,I.*,interpunkce,Z.*,neznámý,X.*"

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "cs_CZ.UTF-8"
}
ATTRIBUTE lemma {
        TYPE "FD_FGD"
        LOCALE "cs_CZ.UTF-8"
}
ATTRIBUTE tag {
        TYPE "FD_FGD"
}
ATTRIBUTE afun {
        TYPE "FD_FGD"
}

ATTRIBUTE p_form {
        TYPE "FD_FGD"
        LOCALE "cs_CZ.UTF-8"
}
ATTRIBUTE p_lemma {
        TYPE "FD_FGD"
        LOCALE "cs_CZ.UTF-8"
}
ATTRIBUTE p_tag {
        TYPE "FD_FGD"
}
ATTRIBUTE p_afun {
        TYPE "FD_FGD"
}

STRUCTURE s {
	ATTRIBUTE id
}

STRUCTURE doc {
	ATTRIBUTE id
        ATTRIBUTE wordcount
}



MAXCONTEXT 50
MAXDETAIL 50

