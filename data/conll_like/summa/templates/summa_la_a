NAME "Summa Theologiae"
PATH "/opt/lindat/kontext-data/corpora/data/parallel/Summa/Summa_la-a"
VERTICAL "/opt/lindat/kontext-data/corpora/vert/parallel/Summa/Summa_la-a"
ENCODING utf-8
INFO "Latin-Czech-English corpus"
LANGUAGE "Latin"

DOCSTRUCTURE seg
STRUCTCTX seg
ALIGNED "summa_en_a,summa_cs_a"
ALIGNSTRUCT seg

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "en_US.UTF-8"
}
ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "en_US.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}
ATTRIBUTE lemma {
        TYPE "FD_FGD"
        LOCALE "en_US.UTF-8"
}

ATTRIBUTE upos {
        TYPE "FD_FGD"
}

ATTRIBUTE xpos {
        TYPE "FD_FGD"
}

ATTRIBUTE feats {
        TYPE "FD_FGD"
        MULTIVALUE y
        MULTISEP "|"
}

ATTRIBUTE parent_id {
        TYPE "FD_FGD"
}

ATTRIBUTE deprel {
        TYPE "FD_FGD"
}
ATTRIBUTE deps {
        TYPE "FD_FGD"
}
ATTRIBUTE misc {
        TYPE "FD_FGD"
}
ATTRIBUTE id {
        TYPE "FD_FGD"
}

STRUCTURE pars {
	ATTRIBUTE id
        ATTRIBUTE wordcount
	DISPLAYBEGIN "  PARS %(id)  "
	DISPLAYEND "  FINIS %(id)  "
	DISPLAYTAG 0
}
STRUCTURE seg {
	ATTRIBUTE id
	DISPLAYBEGIN "%(id)"
	DISPLAYTAG 0
}
STRUCTURE p {
}
STRUCTURE s {
}
STRUCTURE h {
    ATTRIBUTE level {
    }
    DISPLAYCLASS "col0"
    DISPLAYTAG 0
}

STRUCTURE footnote {
    ATTRIBUTE text
    DISPLAYBEGIN "%(text)"
    DISPLAYTAG 0
    DISPLAYCLASS "conc-detail"
}
STRUCTURE i {
}

MAXCONTEXT 50
MAXDETAIL 500

